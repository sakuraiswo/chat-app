


<div class="side-header">
  <div class="side-header-user">
        <%= link_to current_user.name, edit_user_path(current_user) %>
  </div>
  <div class="side-header-btn">
       <%= link_to "チャットを作成する", new_room_path %>
  </div>
</div>
<div class="side-main">
    <% current_user.rooms.each do |room| %>
  <div class="side-main-room">
    <div class="side-main-roomname">
      <a href="#"><%= room.name %></a>
    </div>
  </div>
    <% end %>
</div>


